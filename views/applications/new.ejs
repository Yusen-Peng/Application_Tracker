<h2 class='page-header'>Create a New application</h2>

<!--send a post request to /applications route -->
<!--the form is a multipart form-->
<form action='/applications' method='POST' enctype="multipart/form-data">
    <%- include('_form_field.ejs')%>
    
    <div class="btn-row form-row form-row-end">
        <!--cancel the creation of a new company-->
        <a class="btn btn-dangerous" href='/applications'>Cancel</a>
        <!--submit the form-->
        <button class="btn btn-primary" type='submit'>Create</button>
    </div>

    
    
</form>